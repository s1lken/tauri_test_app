{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "Tauri Button Example",
  "version": "0.1.0",
  "identifier": "com.example.tauri-button",
  "build": {
    "frontendDist": "./dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "title": "Tauri Button Example",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "icon": ["icons/icon.svg"]
  }
}
